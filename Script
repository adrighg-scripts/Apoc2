    Name = "Reset Walk Speed",
    Callback = function()
        toggleWalkSpeed(false)
        WalkSpeedToggle:Set(false)
        NormalWalkSpeedSlider:Set(16)
        SprintWalkSpeedSlider:Set(35)
    end
})

